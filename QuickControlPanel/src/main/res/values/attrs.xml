<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SliderPreference">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="units" format="string" />
    </declare-styleable>
    <declare-styleable name="PagerSwitcher">
        <attr name="direction" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="controlledPager" format="reference" />
    </declare-styleable>
    <declare-styleable name="NonSwipeableViewPager">
        <attr name="enableSwipe" format="boolean"/>
    </declare-styleable>

</resources>